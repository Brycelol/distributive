{
    "Name": "Package checks",
    "Checklist" : [
        {
            "Check" : "installed",
            "Parameters" : ["urxvt"]
        },
        {
            "Check" : "repoExistsUri",
            "Parameters" : ["yum", "http://apt.sw.be/fedora/$releasever/en/$basearch/dag/"]
        },
        {
            "Check" : "repoexists",
            "Parameters" : ["yum", "core"]
        },
        {
            "Check" : "pacmanIgnore",
            "Parameters" : ["pulseaudio"]
        }
    ]
}
