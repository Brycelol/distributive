{
    "Name": "Miscellanious system checks",
    "Checklist" : [
        {
            "Name": "CPU temperature threshold",
            "Check": "temp",
            "Parameters": ["80"]
        },
        {
            "Name": "/sbin/init check",
            "Check" : "running",
            "Parameters": ["/sbin/init"]
        },
        {
            "Check" : "installed",
            "Parameters" : ["urxvt"]
        },
        {
            "Check" : "module",
            "Parameters" : ["ext4"]
        },
        {
            "Check" : "kernelParameter",
            "Parameters" : ["kernel.ctrl-alt-del"]
        },
        {
            "Check" : "phpConfig",
            "Parameters" : ["default_mimetype", "text/html"]
        },
        {
            "Check" : "command",
            "Parameters" : ["sleep .00001"]
        },
        {
            "Check" : "commandOutputMatches",
            "Parameters" : ["echo works", "\\w[aeiou]+(y)*r[kl]s"]
        }
    ]
}
