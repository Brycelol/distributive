{
    "Name": "Miscellanious system checks",
    "Checklist" : [
        {
            "Name": "CPU temperature threshold",
            "Check": "temp",
            "Parameters": ["80"]
        },
        {
            "Name": "/sbin/init check",
            "Check" : "running",
            "Parameters": ["/sbin/init"]
        },
        {
            "Check" : "installed",
            "Parameters" : ["urxvt"]
        },
        {
            "Check" : "module",
            "Parameters" : ["ext4"]
        },
        {
            "Check" : "kernelParameter",
            "Parameters" : ["kernel.ctrl-alt-del"]
        },
        {
            "Check" : "phpConfig",
            "Parameters" : ["default_mimetype", "text/html"]
        },
        {
            "Check" : "memoryUsage",
            "Parameters" : ["90"]
        },
        {
            "Check" : "swapUsage",
            "Parameters" : ["90"]
        },
        {
            "Check" : "cpuUsage",
            "Parameters" : ["95"]
        }
    ]
}
